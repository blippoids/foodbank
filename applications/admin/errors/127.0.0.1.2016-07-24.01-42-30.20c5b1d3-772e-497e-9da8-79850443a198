\r\n                          Field(&lt;span class=&quot;pl-s&quot; style=&quot;box-sizing: border-box; color: rgb(24, 54, 145);&quot;&gt;&lt;span class=&quot;pl-pds&quot; style=&quot;box-sizing: border-box;&quot;&gt;&amp;#39;&lt;/span&gt;img&lt;span class=&quot;pl-pds&quot; style=&quot;box-sizing: border-box;&quot;&gt;&amp;#39;&lt;/span&gt;&lt;/span&gt;,&lt;span class=&quot;pl-s&quot; style=&quot;box-sizing: border-box; color: rgb(24, 54, 145);&quot;&gt;&lt;span class=&quot;pl-pds&quot; style=&quot;box-sizing: border-box;&quot;&gt;&amp;#39;&lt;/span&gt;upload&lt;span class=&quot;pl-pds&quot; style=&quot;box-sizing: border-box;&quot;&gt;&amp;#39;&lt;/span&gt;&lt;/span&gt;))\r\n\r\n&lt;span class=&quot;pl-k&quot; style=&quot;box-sizing: border-box; color: rgb(167, 29, 93);&quot;&gt;from&lt;/span&gt; plugin_thumbnails.thumbnails &lt;span class=&quot;pl-k&quot; style=&quot;box-sizing: border-box; color: rgb(167, 29, 93);&quot;&gt;import&lt;/span&gt; thumbnails          &lt;span class=&quot;pl-c&quot; style=&quot;box-sizing: border-box; color: rgb(150, 152, 150);&quot;&gt;# imports thumbnails plugin&lt;/span&gt;\r\nthumb &lt;span class=&quot;pl-k&quot; style=&quot;box-sizing: border-box; color: rgb(167, 29, 93);&quot;&gt;=&lt;/span&gt; thumbnails(db, &lt;span class=&quot;pl-smi&quot; style=&quot;box-sizing: border-box;&quot;&gt;autodelete&lt;/span&gt;&lt;span class=&quot;pl-k&quot; style=&quot;box-sizing: border-box; color: rgb(167, 29, 93);&quot;&gt;=&lt;/span&gt;&lt;span class=&quot;pl-c1&quot; style=&quot;box-sizing: border-box; color: rgb(0, 134, 179);&quot;&gt;True&lt;/span&gt;)                      &lt;span class=&quot;pl-c&quot; style=&quot;box-sizing: border-box; color: rgb(150, 152, 150);&quot;&gt;# instantiate plugin and crete thumbnails table&lt;/span&gt;\r\nthumb.create(db.mytable.img, (&lt;span class=&quot;pl-c1&quot; style=&quot;box-sizing: border-box; color: rgb(0, 134, 179);&quot;&gt;150&lt;/span&gt;, &lt;span class=&quot;pl-c1&quot; style=&quot;box-sizi